<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Font?>
<GridPane fx:id="gridPane" alignment="center" hgap="10" minHeight="1080.0" minWidth="1920.0" vgap="10"
          xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1"
          fx:controller="app.MainMenuController">
    <columnConstraints>
        <ColumnConstraints/>
        <ColumnConstraints/>
    </columnConstraints>
    <rowConstraints>
        <RowConstraints/>
        <RowConstraints/>
        <RowConstraints/>
        <RowConstraints/>
        <RowConstraints/>
    </rowConstraints>
    <VBox fx:id="vBox" alignment="CENTER" prefHeight="1080.0" prefWidth="1920.0" GridPane.rowIndex="1">
        <Button fx:id="heading" mnemonicParsing="false">
            <padding>
                <Insets bottom="200.0"/>
            </padding>
        </Button>
        <HBox fx:id="hBox" alignment="CENTER" prefHeight="448.0" prefWidth="1234.0">
            <Button fx:id="singlePlayerButton" alignment="CENTER" contentDisplay="CENTER" minHeight="200.0"
                    minWidth="300.0" mnemonicParsing="false" textAlignment="CENTER">
                <font>
                    <Font size="30.0"/>
                </font>
            </Button>
            <Button fx:id="worldBuilderButton" alignment="CENTER" contentDisplay="CENTER" minHeight="200.0"
                    minWidth="300.0" mnemonicParsing="false" textAlignment="CENTER">
                <font>
                    <Font size="30.0"/>
                </font>
                <padding>
                    <Insets left="150.0" right="150.0"/>
                </padding>
            </Button>
            <Button fx:id="exitButton" cancelButton="true" minHeight="200.0" minWidth="300.0"
                    mnemonicParsing="false">
                <font>
                    <Font size="30.0"/>
                </font>
            </Button>
        </HBox>
    </VBox>
</GridPane>
