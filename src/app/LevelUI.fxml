<?xml version="1.0" encoding="UTF-8"?>

<!--suppress JavaFxDefaultTag -->
<?import javafx.scene.control.*?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.*?>
<AnchorPane prefHeight="1080.0" prefWidth="1920.0" xmlns="http://javafx.com/javafx/11.0.1"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="app.LevelController">
    <VBox prefHeight="1080.0" prefWidth="1920.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0"
          AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
        <GridPane alignment="CENTER" prefHeight="1080.0" prefWidth="1920.0">
            <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" maxWidth="1750.0" minWidth="10.0" prefWidth="1714.0"/>
                <ColumnConstraints hgrow="SOMETIMES" maxWidth="955.0" minWidth="10.0" prefWidth="206.0"/>
            </columnConstraints>
            <rowConstraints>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
            </rowConstraints>
            <TitledPane animated="false" prefHeight="510.0" prefWidth="1714.0" text="Levels">
                <content>
                    <ScrollPane fx:id="levels" prefHeight="615.0" prefWidth="1735.0">
                        <content>
                        </content>
                    </ScrollPane>
                </content>
            </TitledPane>
            <TitledPane animated="false" contentDisplay="CENTER" prefHeight="523.0" prefWidth="1714.0"
                        text="own Worlds" GridPane.rowIndex="1">
                <content>
                    <ScrollPane fx:id="ownWorlds" prefHeight="200.0" prefWidth="200.0"/>
                </content>
            </TitledPane>
            <ImageView fitHeight="200.0" fitWidth="145.0" GridPane.columnIndex="1">
                <image>
                    <Image url="@../../imgs/Career.png"/>
                </image>
            </ImageView>
            <ImageView fitHeight="200.0" fitWidth="205.0" GridPane.columnIndex="1" GridPane.rowIndex="1">
                <image>
                    <Image url="@../../imgs/Sandbox.png"/>
                </image>
            </ImageView>
        </GridPane>
        <Button fx:id="backToMainMenu" mnemonicParsing="false" prefHeight="180.0" prefWidth="1925.0"
                text="Back to Main Menu"/>
    </VBox>
</AnchorPane>
